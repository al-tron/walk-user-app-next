{
  "name": "@walk-industries/walk-user-app",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generateBuildTimeStaticData": "dotenv -e .env.local tsx ./scripts/generateBuildTimeStaticData.ts",
    "dev": "npm run generateBuildTimeStaticData && next dev",
    "build": "npm run generateBuildTimeStaticData && next build",
    "vercel-build": "npm run generateBuildTimeStaticData && prisma generate && prisma migrate deploy && prisma db seed && next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier . --write",
    "prisma:migrate": "dotenv -e .env.local prisma migrate dev",
    "prisma:generate": "dotenv -e .env.local prisma generate",
    "prisma:seed": "dotenv -e .env.local prisma db seed",
    "prisma:studio": "dotenv -e .env.local prisma studio",
    "test": "jest"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "prisma": {
    "schema": "src/lib/prisma/schema.prisma",
    "seed": "tsx src/lib/prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2",
    "@heroicons/react": "^2",
    "@hookform/resolvers": "^3",
    "@lucia-auth/adapter-prisma": "^4",
    "@prisma/client": "^5",
    "@sendgrid/mail": "^8",
    "arctic": "^1",
    "clsx": "^2",
    "focus-trap-react": "^10",
    "graphql-request": "^7",
    "gray-matter": "^4",
    "lucia": "^3",
    "next": "14.2.3",
    "ol": "^9",
    "proj4": "^2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7",
    "react-loading-skeleton": "^3",
    "recharts": "^2",
    "remark": "^15",
    "remark-html": "^16",
    "swr": "^2",
    "use-ripple-hook": "^1",
    "zod": "^3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10",
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "^16",
    "@testing-library/user-event": "^14",
    "@types/jest": "^29",
    "@types/node": "20.8.1",
    "@types/proj4": "^2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7",
    "dotenv-cli": "^7",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29",
    "postcss": "^8",
    "prettier": "^3",
    "prettier-plugin-prisma": "^5",
    "prettier-plugin-tailwindcss": "^0",
    "prisma": "^5",
    "tailwind-variants": "^0",
    "tailwindcss": "^3",
    "ts-node": "^10.9.2",
    "tsx": "^4",
    "typescript": "^5"
  }
}
